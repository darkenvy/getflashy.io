{
  "name": "getflashy.io",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "description": "Source code for getflashy.io",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack --watch",
    "prestart": "npm run build",
    "start": "node server.js"
  },
  "engines": {
    "node": "~4.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/require-default-props": 0,
      "max-len": 0,
      "arrow-parens": 0,
      "react/forbid-prop-types": 0
    }
  },
  "dependencies": {
    "async": "^1.5.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "better-sqlite3": "^4.1.4",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "express": "^4.13.4",
    "express-livereload": "0.0.24",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.16.2",
    "marked": "^0.3.6",
    "prop-types": "^15.6.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.2",
    "react-progress-bar-plus": "^1.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "^1.13.5"
  }
}
